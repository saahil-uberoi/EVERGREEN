<!DOCTYPE html>
<html>
<head>
    <link rel='stylesheet' href='~/css/hotspot1.css'>
    
</head>

<body style="font-family:fantasy !important;">
    <!-- bradcam_area_start -->
    <div class="bradcam_area breadcam_bg overlay">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <h3>Grassland</h3>
                </div>
            </div>
        </div>
    </div>
    <!-- bradcam_area_end -->
    <div class="container" style="padding-top:1%; padding-bottom:1%">
        <div class="section_title text-center">
            <h3>Let's see what species you can find in this grassland first</h3>
            <p>Click the hotsposts and discover species</p>
        </div>
        <div style="font-family: Chilanka;">
            <a class="btn bg-theme w3_pvt-link-bnr" id="gamebtn2" style="position:relative; left:4vw; " href='@Url.Action("Select", "Home")'
               role="button">Back</a>
            <a class="btn bg-theme w3_pvt-link-bnr" id="gamebtn2" style="position:relative; left:58vw; " href='@Url.Action("DragDropGrassland", "Home")'
               role="button">Play</a>
        </div>
    </div>
    <div class="container_2">
        <div class="container_2">
            <input type="checkbox" class="input" id="input7" name="inputs1">
            <input type="checkbox" class="input" id="input8" name="inputs1">
            <input type="checkbox" class="input" id="input9" name="inputs1">
            <input type="checkbox" class="input" id="input10" name="inputs1">
            <input type="checkbox" class="input" id="input11" name="inputs1">
            <input type="checkbox" class="input" id="input12" name="inputs1">
            <label for="input7">
                <div id="b7" class="button">1</div>
            </label>
            <label for="input8">
                <div id="b8" class="button">2</div>
            </label>
            <label for="input9">
                <div id="b9" class="button">3</div>
            </label>
            <label for="input10">
                <div id="b10" class="button">4</div>
            </label>
            <label for="input11">
                <div id="b11" class="button">5</div>
            </label>
            <label for="input12">
                <div id="b12" class="button">6</div>
            </label>
            <div id="content7" class="content_2">
                <div style="text-align: right;">
                    <label for="input7" class="cross_2">X</label>
                </div>
                @*<h1>Hoary Sunray</h1>*@
                <img src="~/img/grassland/Giant Gippsland Earthworm.png" alt="Hoary Sunray" style="width: 30vw; height: auto; margin-left:4.5vw;">
                @*<br>
                    <a href="#" class="cta_2">Find out more</a>*@
            </div>

            <div id="content8" class="content_2">
                <div style="text-align: right;">
                    <label for="input8" class="cross_2">X</label>
                </div>
                @*<h1>Scaly Buttons</h1>*@
                <img src="~/img/grassland/Golden-Sun-Moth.png" alt="Scaly Buttons" style="width: 30vw; height: auto; margin-left:4.5vw">
                @*<br>
                    <a href="#" class="cta_2">Find out more</a>*@
            </div>

            <div id="content9" class="content_2">
                <div style="text-align: right;">
                    <label for="input9" class="cross_2">X</label>
                </div>
                @*<h1>Kneed Spear Grass</h1>*@
                <img src="~/img/grassland/European-rabbit.png" alt="Kneed Spear Grass" style="width: 30vw; height: auto; margin-left:4.5vw">
                @*<br>
                    <a href="#" class="cta_2">Find out more</a>*@
            </div>

            <div id="content10" class="content_2">
                <div style="text-align: right;">
                    <label for="input10" class="cross_2">X</label>
                </div>
                @*<h1>Lemon Beauty Heads</h1>*@
                <img src="~/img/grassland/Eltham Copper Butterfly.png" alt="Lemon Beauty Heads" style="width: 30vw; height: auto; margin-left:4.5vw">
                @*<br>
                    <a href="#" class="cta_2">Find out more</a>*@
            </div>

            <div id="content11" class="content_2">
                <div style="text-align: right;">
                    <label for="input11" class="cross_2">X</label>
                </div>
                @*<h1>Rice Flower</h1>*@
                <img src="~/img/grassland/Serrated Tussock.png" alt="Rice Flowera" style="width: 30vw; height: auto; margin-left:4.5vw">
                @*<br>
                    <a href="#" class="cta_2">Find out more</a>*@
            </div>

            <div id="content12" class="content_2">
                <div style="text-align: right;">
                    <label for="input12" class="cross_2">X</label>
                </div>
                @*<h1>Plume Grass</h1>*@
                <img src="~/img/grassland/Southern Brown Bandicoot.png" alt="Plume Grass" style="width: 30vw; height: auto; margin-left:4.5vw">
                @*<br>
                    <a href="#" class="cta_2">Find out more</a>*@
            </div>



        </div>
    </div>
    </div>





</body>
</html>

